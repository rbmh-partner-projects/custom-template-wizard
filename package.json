    "mocha": "^10.0.0",
    "mocha-suppress-logs": "^0.3.1",
    "morgan": "^1.10.0",
    "node-forge": "^1.3.1",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "ora": "^6.1.2",
    "platformsh-config": "^2.4.1",
    "preact": "^10.10.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
