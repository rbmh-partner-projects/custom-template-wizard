    "clui": "^0.3.6",
    "conf": "^10.1.2",
    "configstore": "^6.0.0",
    "cookie-parser": "~1.4.6",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dot": "^1.1.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "figlet": "^1.5.2",
    "fs-extra": "^10.1.0",
