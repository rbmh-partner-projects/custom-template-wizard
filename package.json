    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@rollup/pluginutils": "4.1.2",
    "@types/clear": "^0.1.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/dot": "^1.1.5",
    "@types/express": "^4.17.13",
    "@types/figlet": "^1.5.4",
